   ],
   "source": [
    "num=int(input())\n",
    "mun=num\n",
    "arm=0\n",
    "rem=0\n",
    "while(num>0):\n",
    "    rem=num%10\n",
    "    arm=arm+rem*rem*rem\n",
    "    num=num//10\n",
    "if mun==arm:\n",
    "    print(mun,\"is an armstrong number\")\n",
    "else:\n",
    "    print(mun,\"is not an armstrong number\")\n",
    "\n",
    "\n",
    "\n"
   ]
  }
